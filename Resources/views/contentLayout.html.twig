<!DOCTYPE html>
<html>
  <head>
    <title>
        {% block title%}{% endblock %}
    </title>  
          
    {% block meta %}
        <meta charset="utf-8">
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta name="description" content="The Couchbase PHP Beer-Sample App">
        <meta name="author" content="Couchbase, Inc. 2012">
    {% endblock %}
        
    {% block javascript %}
        <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
        <script>window.jQuery || document.write('<script src="{{ asset('js/jquery.js') }}"><\/script>')</script>
        <script src="{{ asset('js/bootstrap.js') }}"></script>
        
        <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
        <script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
        {% javascripts '@GalaxyBackendBundle/Resources/public/js/*' %}
            <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
    {% endblock %}
        
    {% block css %}
           {#<link rel="stylesheet" href="{{ asset ( 'bundles/galaxybackend/css/docs.css' ) }}"/>
        <link rel="stylesheet" href="{{ asset ( 'bundles/galaxybackend/css/bc-docs.css' ) }}"/>
        <link rel="stylesheet" href="{{ asset ( 'bundles/galaxybackend/css/google-code-prettify.css' ) }}"/>#}
        <link rel="stylesheet" href="{{ asset ('bundles/galaxybackend/css/bootstrap.css') }}">
        <link rel="stylesheet" href="{{ asset ( 'bundles/galaxybackend/css/colorbox.css' ) }}"/>
        <link rel="stylesheet" href="{{ asset ( 'bundles/galaxybackend/css/message.css' ) }}"/>
        <link rel="stylesheet" href="{{ asset ( 'bundles/galaxybackend/css/bc-docs.css' ) }}"/>
        
    {% endblock %}

  </head>
<body data-spy="scroll" data-target=".bs-docs-sidebar">
      
    {% block menu %}
       {% render controller("GalaxyBackendBundle:Default:content", {'route': app.request.get('_route'), 'params': app.request.get('_route_params')})  %}
    
    {% endblock %}


    
  <div class="container" id="cleanContent">

     
            {% block body %} {% endblock %}
  </div>
        <!-- Footer
    ================================================== -->
   {% block footer %}
        <footer class="footer">
          <div class="container">
            <p class="pull-right"> </p>        
            {% render controller("GalaxyBackendBundle:Default:footer") %}
          </div>
        </footer> 
    {% endblock %}
  </body>
</html>